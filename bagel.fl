# Testing non-instantiable classes.
# Sadly there are no bagels to be found in this file.

class A
  inherited(number)
    # The contents of this function don't matter.
    if (number > 14.2778)
      print "Foo"
    else
      print "Your number is unnacceptable"

class B < A
  attribute f = super::inherited(1)

var instance = B()
var callable = instance.f
callable(2)
print callable.receiver == instance
print callable.arity
